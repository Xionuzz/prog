public class EjercicioCinco {

    public static void main(String Args[]) {

	int baseRectangulo = 25;
	int alturaRectangulo = 15;
	System.out.println("Base del rectángulo = "+ baseRectangulo + "cm");
	System.out.println("Altura del rectángulo = "+ alturaRectangulo+ "cm");
	System.out.println("El área del rectángulo es de: " + baseRectangulo*alturaRectangulo+"cm");
	System.out.println("El perímetro del rectángulo es de: " + (baseRectangulo*2+alturaRectangulo*2)+"cm");

    }

}
